{
  "name": "mybartenderai-functions",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && npm run copy:functions",
    "copy:functions": "cpy \"src/**/function.json\" dist",
    "clean": "rimraf dist",
    "start": "npm run build && func start --javascript",
    "start:watch": "func start --typescript",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run"
  },
  "dependencies": {
    "@azure/functions": "^4.5.0",
    "@azure/storage-blob": "^12.18.0",
    "better-sqlite3": "^9.4.3",
    "pg": "^8.12.0",
    "jose": "^5.3.0",
    "openai": "^4.48.0",
    "tmp": "^0.2.3",
    "zstd-codec": "^0.1.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/tmp": "^0.2.6",
    "cpy": "^10.1.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.9.0",
    "pg-mem": "^3.0.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
