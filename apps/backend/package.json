    "tmp": "^0.2.3",
    "zod": "^3.23.8",
    "zstd-codec": "^0.1.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.15.5",
    "@types/tmp": "^0.2.6",
    "copyfiles": "^2.4.1",
    "cpy": "^10.1.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.9.0",
    "pg-mem": "^3.0.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
