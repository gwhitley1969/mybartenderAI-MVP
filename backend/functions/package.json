    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.4.5",
    "vitest": "^2.1.3"
  }
}
